<s:TabBaseWindow x:Class="Acorisoft.FutureGL.Tools.ModuleEditor.MainWindow"
                 x:TypeArguments="vm:AppViewModel"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:local="clr-namespace:Acorisoft.FutureGL.MigaStudio"
                 xmlns:vm="clr-namespace:Acorisoft.FutureGL.Tools.ModuleEditor.ViewModels"
                 xmlns:f="urn:acorisoft/forest/ui"
                 xmlns:fs="urn:acorisoft/forest/shell"
                 xmlns:s="urn:acorisoft/studio"
                 mc:Ignorable="d"
                 Title="MainWindow"
                 MinWidth="1280"
                 MinHeight="720"
                 d:DataContext="{d:DesignInstance Type={x:Type vm:AppViewModel}, IsDesignTimeCreatable=True}">
    <DockPanel Margin="0 36 0 0">
        <Button DockPanel.Dock="Top"
                Content="Refresh"
                Click="ButtonBase_OnClick"
                Height="48"/>
        <ContentControl x:Name="Temp"/>
    </DockPanel>
</s:TabBaseWindow>
