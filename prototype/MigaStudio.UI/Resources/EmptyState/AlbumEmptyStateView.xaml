               Source="{StaticResource EmptyState_Image}"
               Stretch="Uniform" />

        <TextBlock Grid.Row="1"
                   FontSize="24"
                   Margin="0 32 0 16"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   TextWrapping="Wrap">
            <TextBlock.Text>
                <Binding RelativeSource="{RelativeSource AncestorType={x:Type local:AlbumEmptyStateView},Mode=FindAncestor}"
                         Path="Title"
                         Mode="OneWay" />
            </TextBlock.Text>
        </TextBlock>

        <TextBlock Grid.Row="2"
                   Foreground="{f:Brush ForegroundLevel3}"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   TextWrapping="Wrap">
            <TextBlock.Text>
                <Binding RelativeSource="{RelativeSource AncestorType={x:Type local:AlbumEmptyStateView},Mode=FindAncestor}"
                         Path="SubTitle"
                         Mode="OneWay" />
            </TextBlock.Text>
        </TextBlock>

        <f:CallToAction Grid.Row="3"
                        Content="{f:Language global.add}"
                        CornerRadius="24"
                        Margin="0 16"
                        Height="48"
                        Width="160"
                        HorizontalAlignment="Center"
                        Icon="{StaticResource FeatherIcon.Plus}"
                        Palette="Success">
            <f:CallToAction.Command>
                <Binding RelativeSource="{RelativeSource AncestorType={x:Type local:AlbumEmptyStateView},Mode=FindAncestor}"
                         Path="Command"
                         Mode="OneWay" />
            </f:CallToAction.Command>
        </f:CallToAction>
    </Grid>
